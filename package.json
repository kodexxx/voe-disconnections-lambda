{
  "name": "voe",
  "version": "1.0.0",
  "description": "VOE Disconnections lambda for generate calendar with disconnections schedule",
  "main": "main",
  "scripts": {
    "dev": "sls offline",
    "build": "tsc",
    "deploy": "sls deploy",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "deploy:function": "sls deploy function --function",
    "remove": "sls remove",
    "logs": "sls logs --function",
    "logs:tail": "sls logs --function --tail",
    "invoke": "sls invoke --function",
    "invoke:local": "sls invoke local --function",
    "info": "sls info",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "Nazar Rohozhuk",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.602.0",
    "@aws-sdk/client-sqs": "^3.925.0",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/menu": "^1.3.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "grammy": "^1.33.0",
    "hpagent": "^1.2.0",
    "ics": "^3.7.6",
    "node-html-parser": "^6.1.13",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.140",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "serverless-offline": "^14.3.4"
  }
